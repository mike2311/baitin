{
  "name": "baitin-migration-cli",
  "version": "1.0.0",
  "description": "BAITIN Legacy Data Migration CLI Tool",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "main": "dist/main.js",
  "bin": {
    "baitin-migration": "./dist/main.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "tsx src/main.ts",
    "extract": "npm run dev -- extract",
    "load": "npm run dev -- load",
    "validate": "npm run dev -- validate",
    "validate:comprehensive": "tsx src/validator/comprehensive-validator.ts",
    "investigate:orphaned": "tsx investigate-orphaned.ts",
    "reset": "npm run dev -- reset",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "data-quality:report": "tsx src/scripts/generate-data-quality-report.ts",
    "data-quality:track": "tsx src/scripts/generate-data-quality-report.ts",
    "fk:apply-soft": "tsx src/scripts/apply-soft-fk-constraints.ts",
    "fk:validate-strict": "tsx src/scripts/validate-strict-fk-ready.ts",
    "fk:apply-strict": "tsx src/scripts/apply-strict-fk-constraints.ts",
    "master-data:review": "tsx src/scripts/review-and-create-master-data.ts",
    "migration:run": "tsx src/scripts/run-migration.ts"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "winston": "^3.11.0",
    "pg": "^8.11.3",
    "chalk": "^5.3.0",
    "iconv-lite": "^0.6.3",
    "csv-parse": "^5.5.0",
    "dbffile": "^3.1.4"
  },
  "devDependencies": {
    "@types/node": "^20.9.5",
    "@types/pg": "^8.10.9",
    "@types/iconv-lite": "^0.0.1",
    "@types/winston": "^2.4.4",
    "@types/jest": "^29.5.8",
    "vitest": "^1.0.4",
    "ts-node": "^10.9.1",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2"
  }
}

