FUNCTION STRZERO(TEMP_NUMERIC,TEMP_LENGTH)
TEMP_STR=STR(TEMP_NUMERIC,TEMP_LENGTH)
FOR I =1 TO TEMP_LENGTH
    IF SUBSTR(TEMP_STR,I,1)=" "
       IF I=1
          TEMP_STR="0"+SUBSTR(TEMP_STR,I+1,TEMP_LENGTH)
       ELSE
          IF I=TEMP_LENGTH
             TEMP_STR=SUBSTR(TEMP_STR,1,I-1)+"0"
          ELSE
             TEMP_STR=SUBSTR(TEMP_STR,1,I-1)+"0"+SUBSTR(TEMP_STR,I+1,TEMP_LENGTH)
          ENDIF
       ENDIF
    ENDIF
ENDFOR
RETURN (TEMP_STR)
                     
